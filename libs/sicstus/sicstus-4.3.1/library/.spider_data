<?xml version="1.0" encoding="UTF-8"?>
<se.sics.sicstus.spider.data>

  <!-- genmakefile.pl is a perl script -->
  <fileProperties name="genmakefile.pl">
    <property key="ignore" representation="true"/>
    <property key="indexed" representation="false"/>
  </fileProperties>

  <!-- INDEX.pl is a data file (non-Win32)-->
  <fileProperties name="INDEX.pl">
    <property key="indexed" representation="false"/>
  </fileProperties>
  <!-- index.pl is a data file (Win32) -->
  <fileProperties name="index.pl">
    <property key="indexed" representation="false"/>
  </fileProperties>

  <!-- clpq/ contains unused files and should be indexed from clpq.pl -->
  <fileProperties name="clpq">
    <property key="indexed" representation="false"/>
  </fileProperties>
  <!-- clpr/ contains unused files and should be indexed from clpr.pl -->
  <fileProperties name="clpr">
    <property key="indexed" representation="false"/>
  </fileProperties>

</se.sics.sicstus.spider.data>
